{
  "name": "craco-style-resources-loader",
  "version": "1.1.0",
  "description": "A craco plugin to use styles-resources-loader with create-react-app",
  "repository": "github:spacedawwwg/craco-style-resources-loader",
  "main": "index.js",
  "author": "Paul Welsh",
  "license": "MIT",
  "keywords": [
    "craco",
    "create-react-app",
    "react",
    "webpack",
    "css",
    "style-resources-loader"
  ],
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "commitlint": "^13.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "react-scripts": "^4.0.3"
  },
  "peerDependencies": {
    "@craco/craco": "^6.3.0",
    "react-scripts": "^4.0.3"
  },
  "dependencies": {
    "style-resources-loader": "^1.4.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "index.js": [
      "eslint --quiet --fix",
      "git add"
    ]
  }
}
